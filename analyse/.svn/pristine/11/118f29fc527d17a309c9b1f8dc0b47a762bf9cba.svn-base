.mod-layer-drop { position: absolute; top: 40px; width: 80px; border: 1px solid #A5A5A5; background-color: #FFF; box-shadow: 0 2px 3px rgba(0,0,0,.2); border-radius: 3px}
.mod-layer-drop .mod-arr-t { border-width: 6px; border-color: #A5A5A5; position: absolute; top: -6px; left: 34px}
.mod-layer-drop .mod-arr-t span { border-color: #FFF; border-width: 6px; left: -6px}
.mod-layer-drop ul {}
.mod-layer-drop li a { display: block; height: 36px; line-height: 36px; padding-left: 15px; color: #555 }
.mod-layer-drop li a:hover { background-color: #EAEAEA; text-decoration: none}
.mod-arr, .mod-arr span { display: inline-block; position: relative; width: 0; height: 0; font-size: 0; line-height: 0; border-style: dashed; cursor: pointer }
.mod-arr span { position: absolute }
.mod-arr-t, .mod-arr-t span { border-top-width: 0!important; border-style: dashed; border-bottom-style: solid; border-left-color: transparent!important; border-right-color: transparent!important}
.mod-arr-t span {top: 1px}


