/*banner*/
.message_slide{position:relative;width:1200px;height:320px;background:#fff;box-shadow:0 1px 1px rgba(0,0,0,.06);margin:0px auto;}
.message_slide .slide-hd{position:absolute;bottom:0px;height:55px;}
.slide-hd .slide-hd-bg{background:#000;opacity:.6;filter:alpha(opacity=60);position:absolute;z-index:1;width:1200px;height:55px;}
.slide-hd .thumb{position:absolute;width:520px;left:680px;top:-15px;z-index:2;}
.slide-hd .title{position:absolute;/*left:540px;*/width:430px;z-index:2;text-align:left; float:left; padding-left:20px;}
.slide-hd .title a{font-size:26px;font-family:瀵邦喛钂嬮梿鍛寸拨;color:#fff;height:55px;line-height:55px;text-decoration:none;font-weight:400;}
.slide-hd .thumb li{float:left;margin-right:10px;}
.slide-hd .thumb li a{display:block;width:120px;height:60px;overflow:hidden;position:relative;cursor:pointer;}
.slide-hd .thumb a:hover .mask{background:0;opacity:1;filter:0;-webkit-transition:opacity .3s;-moz-transition:opacity .3s;-o-transition:opacity .3s;transition:opacity .3s;}
.slide-hd .thumb li a .mask{display:block;position:absolute;z-index:3;top:0;left:0;width:100%;height:100%;background:#000;opacity:.3;filter:alpha(opacity=30);-moz-transition:background .3s;-webkit-transition:background .3s;-o-transition:background .3s;transition:background .3s;}
.slide-hd .thumb .selected a .mask{width:114px;height:54px;border:3px solid #db253e;background:0;opacity:1;filter:0;-webkit-transition:opacity .3s;-moz-transition:opacity .3s;-o-transition:opacity .3s;transition:opacity .3s;}
.message_slide .slide-bd{width:1200px;height:320px;overflow:hidden;}
.message_slide .slide-bd .pic-list{width:5000px;height:370px;}

.message_slide .slide-btn a{position:absolute;top:170px;text-indent:-999em;display:block;height:80px;width:28px;background-image:url(../../images/front/arrow.png);background-repeat:no-repeat;_background-image:none;}
.message_slide .slide-btn .prev{left:25px;background-position:0 0;}
.message_slide .slide-btn .next{right:25px;background-position:0 -120px;}
.message_slide .slide-btn .prev.selected{background-position:-80px 0;}
.message_slide .slide-btn .next.selected{background-position:-80px -120px;}
.message_slide .slide-btn .prev:hover{background-position:-160px 0;}
.message_slide .slide-btn .next:hover{background-position:-160px -120px;}
.message_slide .slide-btn a div{position:relative;height:80px;width:28px;}
.message_slide .slide-btn a span{display:block;height:80px;width:28px;top:0;left:0;position:absolute;cursor:pointer;}
.message_slide .slide-btn .mask{background:#fff;filter:alpha(opacity=0);z-index:2;}
.message_slide .slide-btn .selected .mask{filter:alpha(opacity=50);}
.message_slide .slide-btn a:hover .mask{filter:alpha(opacity=70);}
.message_slide .slide-btn .prev .arrow{background:url(../../images/front/jiantou.png) no-repeat 0 -120px;z-index:3;}
.message_slide .slide-btn .next .arrow{background:url(../../images/front/jiantou.png) no-repeat 0 0;z-index:3;}

